
@{
    ViewBag.Title = "Recuperar";
}

@using (Html.BeginForm("Recuperar", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="row">
        <div class="col-md-4">
            <h2>Recuperar arquivo</h2>
            <hr />
            <h2>@TempData["Sucesso"]</h2>

            <input type="submit" class="btn btn-default" value="Recuperar" />
        </div>
    </div>
}